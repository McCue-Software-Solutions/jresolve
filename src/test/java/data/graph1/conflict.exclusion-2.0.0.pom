<?xml version="1.0" encoding="UTF-8" ?>
<project xmlns="http://maven.apache.org/POM/4.0.0">
    <modelVersion>4.0.0</modelVersion>
    <groupId>jresolve.test</groupId>
    <artifactId>conflict.exclusion</artifactId>
    <version>2.0.0</version>
    <name>conflict.exclusion</name>
    <description>Used to represent a dep with multiple dependencies of which some are excluded</description>

    <dependencies>
        <dependency>
            <groupId>jresolve.test</groupId>
            <artifactId>first.solo.dep</artifactId>
            <version>1.0.0</version>
        </dependency>
        <dependency>
            <groupId>jresolve.test</groupId>
            <artifactId>first.leaf.dep</artifactId>
            <version>3.1.0</version>
            <exclusions>
                <exclusion>
                    <groupId>jresolve</groupId>
                    <artifactId>second.leaf.dep</artifactId>
                </exclusion>
            </exclusions>
        </dependency>
        <dependency>
            <groupId>jresolve.test</groupId>
            <artifactId>second.leaf.dep</artifactId>
            <version>3.3.3</version>
            <exclusions>
                <exclusion>
                    <groupId>jresolve</groupId>
                    <artifactId>first.solo.dep</artifactId>
                </exclusion>
            </exclusions>
        </dependency>
    </dependencies>
</project>